<div class="product-images-carousel">
<% product.gallery.images.each do |image| %>
    <div class="product-images-carousel-item">
    
      <%= render(ImageComponent.new(
        image: image,
        size: :large,
        itemprop: "image",
        data: { js: 'product-main-image' },
        class: 'carousel-img'
      )) %>
      
    </div>
    <% end %>


</div>
